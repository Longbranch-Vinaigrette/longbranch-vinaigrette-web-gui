{
	"devtools": {
		"importance": "core",
		"name": "DevTools Website",
		"release": false,
		"repositoryName": "dev-tools-website",
		"version": "v1.0.0",
		"commands": {
			"setup": "npm install",
			"build": "npm run build",
			"start": "npx next start -p 37001"
		},
		"env": {
			"fileName": ".env",
			"dotenv5": false,
			"variables": [
				{
					"formalName": "Server port",
					"varName": "PORT",
					"description": "Server port, defaults to 37000."
				}
			]
		}
	}
}
